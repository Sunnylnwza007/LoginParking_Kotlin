<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/loginFragment">

    <fragment
        android:id="@+id/loginFragment"
        android:name="buu.informatics.s59160090.loginparking.LoginFragment"
        android:label="LoginFragment"
        tools:layout="@layout/fragment_login">
        <action
            android:id="@+id/action_loginFragment_to_parkingFragment"
            app:destination="@+id/parkingFragment" />
    </fragment>
    <fragment
        android:id="@+id/parkingFragment"
        android:name="buu.informatics.s59160090.loginparking.ParkingFragment"
        android:label="ParkingFragment"
        tools:layout="@layout/fragment_parking"/>
    <fragment
        android:id="@+id/infoFragment"
        android:name="buu.informatics.s59160090.loginparking.InfoFragment"
        android:label="InfoFragment"
        tools:layout="@layout/fragment_info"/>/>
</navigation>